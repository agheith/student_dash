<div class="container">

    <div class="row">

        <h1>Listing <%= @dojos.count %> Dojos</h1>

        <div class="col-md-12">
            <table class="highlight table">
              <thead>
                <tr>
                  <th>Branch</th>
                  <th>Street</th>
                  <th>City</th>
                  <th>State</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>


                  <% @dojos.each do |dojo| %>


                <tr>
                  <td><%= dojo.branch %></td>
                  <td>st</td>
                  <td><%= dojo.city %></td>
                  <td><%= dojo.state %></td>
                  <td>
                      <a href='/dojos/<%= dojo.id %>'>Show</a> | <a href="/dojos/<%= dojo.id %>/edit">Edit</a> | <a href="/dojos/<%= dojo.id %>" data-method="delete">Destroy</a>


                  </td>
                </tr>



                <% end %>


            </tbody>
            </table>

            <hr>

            <a href="/dojos/new">New Dojo</a>
        </div>


    </div>


</div>
